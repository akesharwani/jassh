<configuration>

  <appender name="STDOUT"
   class="ch.qos.logback.core.ConsoleAppender">
   <encoder>
     <pattern>
        %-5level - %msg%n
      </pattern>
    </encoder>
  </appender>


  <logger name="fr.janalyse.ssh" level="WARN"/>


  <root level="WARN">
     <appender-ref ref="STDOUT" />
     <appender-ref ref="FILE" />
  </root>

</configuration>
